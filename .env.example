# PostgreSQL connection string
DATABASE_URL="postgresql://<user>:<password>@<host>/<database>?sslmode=require"

# UploadThing API key (used for file uploads)
UPLOADTHING_TOKEN="<your-uploadthing-token>"

# Resend API key for sending transactional emails
RESEND_API_KEY="<your-resend-api-key>"

# Clerk public key (safe to expose to frontend)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY="pk_test_..."
# Clerk secret key (keep private)
CLERK_SECRET_KEY="sk_test_..."

# Current application cycle ('open', 'closed')
NEXT_PUBLIC_APPLICATION_CYCLE="open"
